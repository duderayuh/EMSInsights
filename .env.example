# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/ems_insight

# Rdio Scanner Configuration
RDIO_SCANNER_URL=http://hoosierems.org:3001
RDIO_SCANNER_API_KEY=your_rdio_scanner_api_key_here
RDIO_SCANNER_SYSTEMS=MESA
RDIO_SCANNER_TALKGROUPS=10202,10244

# SDRTrunk Configuration
SDR_HOST=localhost
SDR_PORT=9999
SDR_PIPE_PATH=/tmp/sdr_audio.fifo

# Whisper Configuration
WHISPER_MODEL=base
USE_WHISPER_API=false
OPENAI_API_KEY=your_openai_api_key_here

# Server Configuration
NODE_ENV=development
PORT=5000

# Audio Processing
AUDIO_SEGMENT_DIR=./audio_segments
AUDIO_CHUNK_SIZE=30

# NLP Configuration
KEYWORDS_CONFIG_PATH=./config/keywords.json

# WebSocket Configuration
WS_HEARTBEAT_INTERVAL=30000

# System Configuration
LOG_LEVEL=info
